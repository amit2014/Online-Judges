#include <set>
//BEGIN_TEMPLATE_BY_PRATYUSH_VERMA
#define INF (1<<31)-1
#define MAX(i,j) (i)>(j)?(i):(j)
#define MIN(i,j) (i)<(j)?(i):(j)
#define REP(i,a) for((i)=0;(i)<(a);(i)++)
#define REP_(i,a) for((i)=0;(i)<=(a);(i)++)
#define FOR(i,a,b) for((i)=(a);(i)<(b);(i)++)
#define FOR_(i,a,b) for((i)=(a);(i)<=(b);(i)++)
#define VAR(V,init) __typeof(init) V=(init)
#define FOREACH(I,C) for(VAR(I,(C).begin());I!=(C).end();I++)
#define ALL(x) (x).begin(),(x).end()
#define SIZE(x) ((int)(x.size()))
#define LENGTH(x) ((int)(x.length()))
#define SZ(x) sizeof(x)
#define MEM(m,i) memset((m),(i),SZ(m))
#define PB(x,y) (x).push_back(y)
#define MP(x,y) make_pair(x,y)
#define V(x) vector < x >

typedef pair<int,int>   PII;
typedef pair<char,int>  PCI;
typedef pair<int,PII>   TRI;
typedef V( int )        VI;
typedef V( PII )        VII;
typedef V( TRI )        VTRI;
typedef V( string )     VS;
typedef long long       LL;
typedef long double     LD;

inline string i2s(int number) { stringstream ss; ss << number; return ss.str(); }
//END_TEMPLATE_BY_PRATYUSH_VERMA
bool arr[10000001];
LL pos[10000001];
//vector<vector<int> > v;
LL weight[10000001];

int main()
{


    MEM(weight,0);
    MEM(pos,0);
    MEM(arr,1);
    long long int i,j,k,p=1,q;

        FOR(i,2,10000001)
    {
        if (arr[i])
        {
            pos[i]=p++;
            weight[i]=i;
           // cout<<weight[i]<<" "<<i<<endl;
            //getchar();
            for(j=i*2;j<10000001;j=j+i)
            arr[j]=0;
        }
    }
cout<<pos[3]<<endl;
    for (int i=2;i<10000001;i++) {
        if (weight[i]==0) {
            long long k=i-1;
            for (;k>=2;k--) {
                if (arr[k]==true) {
                    if (i%k==0) {
                        cout<<k<<" GDGF" <<endl;
                        getchar();
                        LL j=i/k;
                        cout<<j<<endl;
                        if (arr[j]&&j!=k){
                            cout<<"DS"<<endl;
                            LL t=pos[k];
                            LL power=1;
                            
                            while (t--) {
                                power=power*k;
                            }
                            weight[i]=weight[j]*power;
                        }
                        else weight[i]=weight[j];
                        break;
                    }
                }
            }
        }
        cout<<i<<" "<<weight[i]<<endl;
        getchar();
    }

while (cin>>q)
cout<<weight[q]<<endl;



            
    
    
    


    
    
    
	system("pause"); //Windows Only
	return 0;
}
